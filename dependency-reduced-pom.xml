<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>RTB4FREE</groupId>
  <artifactId>RTB5</artifactId>
  <name>RTB4FREE</name>
  <version>0.0.1-SNAPSHOT</version>
  <description>RTB 2.8 DSP</description>
  <url>http://rtb4free.com</url>
  <organization>
    <name>Jacamars</name>
    <url>http://rtb4free.com</url>
  </organization>
  <build>
    <sourceDirectory>src</sourceDirectory>
    <resources>
      <resource>
        <directory>src</directory>
        <excludes>
          <exclude>**/*.java</exclude>
        </excludes>
      </resource>
    </resources>
    <plugins>
      <plugin>
        <artifactId>maven-surefire-plugin</artifactId>
        <version>2.16</version>
        <configuration>
          <reuseForks>false</reuseForks>
          <forkCount>1</forkCount>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <version>3.2.4</version>
        <executions>
          <execution>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
            <configuration>
              <artifactSet>
                <excludes />
              </artifactSet>
              <transformers>
                <transformer />
              </transformers>
            </configuration>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>
  <profiles>
    <profile>
      <id>allow-snapshots</id>
      <repositories>
        <repository>
          <releases>
            <enabled>false</enabled>
          </releases>
          <snapshots />
          <id>snapshots-repo</id>
          <url>https://oss.sonatype.org/content/repositories/snapshots</url>
        </repository>
      </repositories>
    </profile>
  </profiles>
  <dependencies>
    <dependency>
      <groupId>org.apache.zookeeper</groupId>
      <artifactId>zookeeper</artifactId>
      <version>3.5.6</version>
      <type>pom</type>
      <scope>compile</scope>
      <exclusions>
        <exclusion>
          <artifactId>jmxri</artifactId>
          <groupId>com.sun.jmx</groupId>
        </exclusion>
        <exclusion>
          <artifactId>jmxtools</artifactId>
          <groupId>com.sun.jdmk</groupId>
        </exclusion>
        <exclusion>
          <artifactId>jms</artifactId>
          <groupId>javax.jms</groupId>
        </exclusion>
      </exclusions>
    </dependency>
  </dependencies>
  <reporting>
    <plugins>
      <plugin>
        <artifactId>maven-surefire-report-plugin</artifactId>
        <version>2.19.1</version>
      </plugin>
    </plugins>
  </reporting>
  <properties>
    <bucket4j.version>4.9.0</bucket4j.version>
    <graalvm.version>19.1.0</graalvm.version>
    <aws.sdk.version>1.11.880</aws.sdk.version>
    <hazelcast-kubernetes.version>2.1</hazelcast-kubernetes.version>
    <jamm.version>RELEASE</jamm.version>
    <jackson.version>2.12.1</jackson.version>
    <zookeeper.version>3.5.6</zookeeper.version>
    <postgres.version>9.1-901-1.jdbc4</postgres.version>
    <elastic.rest.version>7.6.1</elastic.rest.version>
    <hazelcast.version>4.0.3</hazelcast.version>
    <jetty.server.version>9.4.38.v20210224</jetty.server.version>
    <junit.version>4.13.1</junit.version>
    <grok.version>0.1.9</grok.version>
    <maxmind.version>2.12.0</maxmind.version>
    <javax.persistence.version>1.0.2</javax.persistence.version>
    <maven.compiler.source>11</maven.compiler.source>
    <redis.version>2.9.0</redis.version>
    <maven.plugin.version>2.19.1</maven.plugin.version>
    <javax.servlet.version>4.0.0</javax.servlet.version>
    <kafka.version>0.10.2.2</kafka.version>
    <openrtb.core.version>1.5.1</openrtb.core.version>
    <mapdb.version>3.0.5</mapdb.version>
    <socket.io.version>1.0.10</socket.io.version>
    <google.protobuf.version>3.3.1</google.protobuf.version>
    <maven.compiler.target>11</maven.compiler.target>
    <http.client.version>4.5.3</http.client.version>
    <antlr.runtime.version>4.7</antlr.runtime.version>
    <commons.pool2.version>2.5.0</commons.pool2.version>
    <slf4j.api.version>1.7.25</slf4j.api.version>
    <commons.io.version>2.7</commons.io.version>
  </properties>
</project>
